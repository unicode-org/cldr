<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision: $"/>
		<generation date="$Date: $"/>
		<language type="sr"/>
	</identity>
	<behavior>
		<casing type="calendar-field">lowercase</casing>
		<casing type="day-format-except-narrow">lowercase</casing>
		<casing type="day-narrow">lowercase</casing>
		<casing type="day-standalone-except-narrow">lowercase</casing>
		<casing type="displayName">uppercase</casing>
		<casing type="displayName-count">lowercase</casing>
		<casing type="era-abbr">uppercase</casing>
		<casing type="era-name">uppercase</casing>
		<casing type="era-narrow">uppercase</casing>
		<casing type="key">uppercase</casing>
		<casing type="language">uppercase</casing>
		<casing type="metazone-long">uppercase</casing>
		<casing type="month-narrow">lowercase</casing>
		<casing type="quarter-abbreviated">uppercase</casing>
		<casing type="quarter-format-wide">uppercase</casing>
		<casing type="quarter-standalone-wide">uppercase</casing>
		<casing type="script">lowercase</casing>
		<casing type="territory">uppercase</casing>
		<casing type="unit-pattern">lowercase</casing>
		<casing type="variant">uppercase</casing>
		<casing type="zone-exemplarCity">uppercase</casing>
	</behavior>
</ldml>
