<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$" />
		<generation date="$Date$" />
		<language type="kk"/>
	</identity>
	<collations validSubLocales="kk_KZ">
		<collation type="standard">
			<suppress_contractions>[АаӘәГгЕеЖжЗзІіОоӨөКкУуЧчЫыЭэѴѵ]</suppress_contractions>
			<!--
				List of UCA contractions remaining after suppression:

				Ё 0415 0308 CYRILLIC CAPITAL LETTER IO
				ё 0435 0308 CYRILLIC SMALL LETTER IO
				Й 0418 0306 CYRILLIC CAPITAL LETTER SHORT I
				й 0438 0306 CYRILLIC SMALL LETTER SHORT I
			-->
			<rules>
				<reset>Е</reset>
					<p>ё</p>
					<t>Ё</t>
				<reset>Ұ</reset>
					<p>ү</p>
					<t>Ү</t>
				<reset before="primary">ь</reset>
					<p>і</p>
					<t>І</t>
			</rules>
		</collation>
	</collations>
</ldml>
