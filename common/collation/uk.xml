<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="uk" /> 
	</identity>
	<collations validSubLocales="uk_UA">
		<collation type="standard">
			<suppress_contractions>[АаӘәГгЕеЖжЗзОоӨөКкУуЧчЫыЭэѴѵ]</suppress_contractions>
			<!-- List of UCA contractions remaining after suppression:

			Ї 0406 0308 CYRILLIC CAPITAL LETTER YI
			ї 0456 0308 CYRILLIC SMALL LETTER YI
			Й 0418 0306 CYRILLIC CAPITAL LETTER SHORT I
			й 0438 0306 CYRILLIC SMALL LETTER SHORT I
			-->
			<rules>
			    <reset>Г</reset>
			        <p>ґ</p>
			        <t>Ґ</t>
			    <reset>я</reset>
			        <p>ь</p>
			        <t>Ь</t>
			</rules>
		</collation>
	</collations>
</ldml>
