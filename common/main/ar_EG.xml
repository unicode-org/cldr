<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.3/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="ar"/>
		<territory type="EG"/>
	</identity>
	<delimiters>
		<quotationStart draft="true">‘</quotationStart>
		<quotationEnd draft="true">’</quotationEnd>
		<alternateQuotationStart draft="true">&quot;</alternateQuotationStart>
		<alternateQuotationEnd draft="true">&quot;</alternateQuotationEnd>
	</delimiters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<week>
					<weekendStart day="fri" draft="true"/>
					<weekendEnd day="sat" draft="true"/>
				</week>
				<eras>
					<eraNames>
						<era type="0" draft="true">قبل الميلاد</era>
						<era type="1" draft="true">ميلادي</era>
					</eraNames>
				</eras>
			</calendar>
		</calendars>
	</dates>
</ldml>

