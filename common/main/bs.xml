<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.3/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="bs"/>
		<!-- <territory type="BA"/> -->
	</identity>
	<delimiters>
		<quotationStart draft="true">‘</quotationStart>
		<quotationEnd draft="true">’</quotationEnd>
		<alternateQuotationStart draft="true">“</alternateQuotationStart>
		<alternateQuotationEnd draft="true">”</alternateQuotationEnd>
	</delimiters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<months>
					<monthContext type="format">
						<monthWidth type="abbreviated">
							<month type="1" draft="true">Jan</month>
							<month type="2" draft="true">Feb</month>
							<month type="3" draft="true">Mar</month>
							<month type="4" draft="true">Apr</month>
							<month type="5" draft="true">Maj</month>
							<month type="6" draft="true">Jun</month>
							<month type="7" draft="true">Jul</month>
							<month type="8" draft="true">Avg</month>
							<month type="9" draft="true">Sep</month>
							<month type="10" draft="true">Okt</month>
							<month type="11" draft="true">Nov</month>
							<month type="12" draft="true">Dec</month>
						</monthWidth>
						<monthWidth type="wide">
							<month type="1" draft="true">Januar</month>
							<month type="2" draft="true">Februar</month>
							<month type="3" draft="true">Mart</month>
							<month type="4" draft="true">April</month>
							<month type="5" draft="true">Maj</month>
							<month type="6" draft="true">Juni</month>
							<month type="7" draft="true">Juli</month>
							<month type="8" draft="true">Avgust</month>
							<month type="9" draft="true">Septembar</month>
							<month type="10" draft="true">Oktobar</month>
							<month type="11" draft="true">Novembar</month>
							<month type="12" draft="true">Decembar</month>
						</monthWidth>
					</monthContext>
				</months>
				<days>
					<dayContext type="format">
						<dayWidth type="abbreviated">
							<day type="sun" draft="true">Ned</day>
							<day type="mon" draft="true">Pon</day>
							<day type="tue" draft="true">Uto</day>
							<day type="wed" draft="true">Sri</day>
							<day type="thu" draft="true">Čet</day>
							<day type="fri" draft="true">Pet</day>
							<day type="sat" draft="true">Sub</day>
						</dayWidth>
						<dayWidth type="wide">
							<day type="sun" draft="true">Nedjelja</day>
							<day type="mon" draft="true">Ponedjeljak</day>
							<day type="tue" draft="true">Utorak</day>
							<day type="wed" draft="true">Srijeda</day>
							<day type="thu" draft="true">Četvrtak</day>
							<day type="fri" draft="true">Petak</day>
							<day type="sat" draft="true">Subota</day>
						</dayWidth>
					</dayContext>
				</days>
				<week>
					<firstDay day="mon" draft="true"/>
				</week>
				<eras>
					<eraAbbr>
						<era type="0" draft="true">BC</era>
						<era type="1" draft="true">AD</era>
					</eraAbbr>
				</eras>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<symbols>
			<decimal draft="true">,</decimal>
			<group draft="true">.</group>
		</symbols>
		<currencies>
			<!-- <default type="BAM"/> -->
			<currency type="BAM">
				<displayName draft="true">Konvertibilna marka</displayName>
				<symbol draft="true">KM</symbol>
			</currency>
		</currencies>
	</numbers>
</ldml>

