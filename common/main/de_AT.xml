<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.6/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="de"/>
		<territory type="AT"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<months>
					<monthContext type="format">
						<monthWidth type="abbreviated">
							<month type="1">Jän</month>
							<month type="3">Mär</month>
						</monthWidth>
						<monthWidth type="wide">
							<month type="1">Jänner</month>
						</monthWidth>
					</monthContext>
				</months>
				<dateFormats>
					<dateFormatLength type="full">
						<dateFormat>
							<pattern>EEEE, dd. MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="long">
						<dateFormat>
							<pattern>dd. MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<dateTimeFormats>
					<intervalFormats>
						<intervalFormatItem id="MMMEd">
							<greatestDifference id="M" draft="unconfirmed">E, dd. MMM - E, dd. MMM</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">E, dd. - E, dd. MMM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MMMd">
							<greatestDifference id="M" draft="unconfirmed">dd. MMM - dd. MMM</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">dd.-dd. MMM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMMEd">
							<greatestDifference id="M" draft="unconfirmed">E, dd. MMM - E, dd. MMM yyyy</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">E, dd. - E, dd. MMM yyyy</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">E, dd. MMM yyyy - E, dd. MMM yyyy</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMMd">
							<greatestDifference id="M" draft="unconfirmed">dd. MMM - dd. MMM yyyy</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">dd.-dd. MMM yyyy</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">dd. MMM yyyy - dd. MMM yyyy</greatestDifference>
						</intervalFormatItem>
					</intervalFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>¤ #,##0.00</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>

