<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="fr"/>
		<territory type="CH"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="full">
						<dateFormat>
							<pattern>EEEE, d MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>dd.MM.yy</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>HH.mm:ss' h' z</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats draft="true">
						<dateFormatItem id="1">MM-dd</dateFormatItem>
						<dateFormatItem id="2">d. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="3">d. MMM yyyy</dateFormatItem>
						<dateFormatItem id="4">EEEE,d. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="5">EEE, d. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="6">EEE, d. MMM yy</dateFormatItem>
						<dateFormatItem id="7">d. MMM yy</dateFormatItem>
						<dateFormatItem id="8">EEEE,dd. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="9">dd.MM.yy</dateFormatItem>
						<dateFormatItem id="10">dd.MM.yyyy</dateFormatItem>
						<dateFormatItem id="11">QQQQ yy</dateFormatItem>
						<dateFormatItem id="12">MMMM</dateFormatItem>
						<dateFormatItem id="13">dd.MMM</dateFormatItem>
						<dateFormatItem id="14">MM.yy</dateFormatItem>
						<dateFormatItem id="15">EEE dd.MMM yy</dateFormatItem>
						<dateFormatItem id="16">d.MM.yy</dateFormatItem>
						<dateFormatItem id="17">ww</dateFormatItem>
						<dateFormatItem id="18">yyyy-MM-dd</dateFormatItem>
						<dateFormatItem id="19">yy-MM-dd</dateFormatItem>
						<dateFormatItem id="20">mm:ss.SS</dateFormatItem>
						<dateFormatItem id="21">KK:mm:ss a</dateFormatItem>
						<dateFormatItem id="22">KK:mm a</dateFormatItem>
						<dateFormatItem id="23">HH:mm:ss</dateFormatItem>
						<dateFormatItem id="24">HH:mm</dateFormatItem>
						<dateFormatItem id="25">dd.MM.yyyy HH:mm:ss</dateFormatItem>
						<dateFormatItem id="26">dd.MM.yy HH:mm</dateFormatItem>
					</availableFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<symbols>
			<decimal>.</decimal>
			<group>'</group>
		</symbols>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>¤ #,##0.00;¤-#,##0.00</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>

