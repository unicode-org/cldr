<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.1/ldml.dtd">
<ldml>
	<identity>
		<version number="1.2"/>
		<generation date="2004-08-17" />
		<language type="iw" /> 
		<territory type="IL" /> 
	</identity>
	<characters>
	</characters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<week>
					<weekendStart draft="true" day="fri" time="00:00"/>
					<weekendEnd draft="true" day="sun" time="00:00"/>
				</week>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<decimalFormats>
			<decimalFormatLength >
				<decimalFormat >
					<pattern>#,##0.###;-#,##0.###</pattern>
				</decimalFormat>
			</decimalFormatLength >
		</decimalFormats>
		<scientificFormats>
			<scientificFormatLength >
				<scientificFormat >
					<pattern>#E0</pattern>
				</scientificFormat>
			</scientificFormatLength >
		</scientificFormats>
		<percentFormats>
			<percentFormatLength >
				<percentFormat >
					<pattern>#,##0%</pattern>
				</percentFormat>
			</percentFormatLength >
		</percentFormats>
		<currencyFormats>
			<currencyFormatLength >
				<currencyFormat >
					<pattern>#,##0.00 ¤;-#,##0.00 ¤</pattern>
				</currencyFormat>
			</currencyFormatLength >
		</currencyFormats>
	</numbers>
</ldml>
