<ldml>
	<identity>
		<version number="$Revision$"/>
		<language type="pis"/>
	</identity>
	<localeDisplayNames>
		<localeDisplayPattern>
			<localePattern>{0} ({1})</localePattern>
			<localeSeparator>{0}, {1}</localeSeparator>
		</localeDisplayPattern>
		<languages>
			<language type="en">Ingglis</language>
			<language type="pis">Pijin</language>
		</languages>
		<scripts>
			<script type="Latn">Latin</script>
		</scripts>
		<territories>
			<territory type="SB">Solomon Aelan</territory>
		</territories>
		<measurementSystemNames>
			<measurementSystemName type="metric">metrik</measurementSystemName>
			<measurementSystemName type="UK">UK</measurementSystemName>
			<measurementSystemName type="US">US</measurementSystemName>
		</measurementSystemNames>
		<codePatterns>
			<codePattern type="language">Langguis: {0}</codePattern>
			<codePattern type="script">Raeting kaen: {0}</codePattern>
			<codePattern type="territory">Rijin: {0}</codePattern>
		</codePatterns>
	</localeDisplayNames>
	<characters>
		<exemplarCharacters>[a b d e f g h i j k l m n o p r s t u v w y]</exemplarCharacters>
		<exemplarCharacters type="numbers">[\- ‑ / # % + 0 1 2 3 5 6 7 8 9]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[\- ‑ , ; \: ! ? . ' ‘ ’ &quot; “ ” ( ) + =]</exemplarCharacters>
	</characters>
	<delimiters>
		<quotationStart>“</quotationStart>
		<quotationEnd>”</quotationEnd>
		<alternateQuotationStart>‘</alternateQuotationStart>
		<alternateQuotationEnd>’</alternateQuotationEnd>
	</delimiters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<months>
					<monthContext type="format">
						<monthWidth type="wide">
							<month type="1">Januare</month>
							<month type="2">Febuare</month>
							<month type="3">Mas</month>
							<month type="4">Eprel</month>
							<month type="5">Mei</month>
							<month type="6">Jun</month>
							<month type="7">Julae</month>
							<month type="8">Ogus</month>
							<month type="9">Septemba</month>
							<month type="10">Oktoba</month>
							<month type="11">Novemba</month>
							<month type="12">Disemba</month>
						</monthWidth>
					</monthContext>
				</months>
				<days>
					<dayContext type="format">
						<dayWidth type="wide">
							<day type="sun">Sande</day>
							<day type="mon">Mande</day>
							<day type="tue">Tiusde</day>
							<day type="wed">Wenesde</day>
							<day type="thu">Tosde</day>
							<day type="fri">Fraede</day>
							<day type="sat">Satade</day>
						</dayWidth>
					</dayContext>
				</days>
				<dayPeriods>
					<dayPeriodContext type="format">
						<dayPeriodWidth type="wide">
							<dayPeriod type="am">AM</dayPeriod>
							<dayPeriod type="pm">PM</dayPeriod>
						</dayPeriodWidth>
					</dayPeriodContext>
				</dayPeriods>
				<dateTimeFormats>
					<intervalFormats>
						<intervalFormatFallback>{0} – {1}</intervalFormatFallback>
					</intervalFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
		<timeZoneNames>
			<gmtFormat>GMT {0}</gmtFormat>
			<gmtZeroFormat>GMT</gmtZeroFormat>
			<regionFormat>{0} Taem</regionFormat>
			<regionFormat type="daylight">{0} Delaet Taem</regionFormat>
			<regionFormat type="standard">{0} Standad Taem</regionFormat>
			<fallbackFormat>{1} ({0})</fallbackFormat>
			<metazone type="GMT">
				<long>
					<standard>Grinis Min Taem</standard>
				</long>
			</metazone>
		</timeZoneNames>
	</dates>
	<numbers>
		<defaultNumberingSystem>latn</defaultNumberingSystem>
		<otherNumberingSystems>
			<native>latn</native>
		</otherNumberingSystems>
		<symbols numberSystem="latn">
			<decimal>.</decimal>
			<group>,</group>
			<percentSign>%</percentSign>
			<plusSign>+</plusSign>
			<minusSign>-</minusSign>
		</symbols>
		<decimalFormats numberSystem="latn">
			<decimalFormatLength>
				<decimalFormat>
					<pattern>#,##0.###</pattern>
				</decimalFormat>
			</decimalFormatLength>
		</decimalFormats>
		<currencies>
			<currency type="SBD">
				<symbol>$</symbol>
			</currency>
		</currencies>
	</numbers>
</ldml>
