<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="sr"/>
		<script type="Latn"/>
		<territory type="ME"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="generic">
				<dateFormats>
					<dateFormatLength type="long">
						<dateFormat>
							<pattern>d.MM.y. G</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<dateTimeFormats>
					<intervalFormats>
						<intervalFormatItem id="MMMd">
							<greatestDifference id="d" draft="unconfirmed">d.MM - d.MM</greatestDifference>
							<greatestDifference id="M" draft="unconfirmed">d.MM - d.MM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MMMEd">
							<greatestDifference id="d" draft="unconfirmed">E, d.MM - E, d.MM</greatestDifference>
							<greatestDifference id="M" draft="unconfirmed">E, d.MM - E, d.MM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMM">
							<greatestDifference id="M" draft="unconfirmed">MM.y - MM.y</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">MM.y - MM.y</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMMd">
							<greatestDifference id="d" draft="unconfirmed">d.MM.y. - d.MM.y.</greatestDifference>
							<greatestDifference id="M" draft="unconfirmed">d.MM.y. - d.MM.y.</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">d.MM.y. - d.MM.y.</greatestDifference>
						</intervalFormatItem>
					</intervalFormats>
				</dateTimeFormats>
			</calendar>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="long">
						<dateFormat>
							<pattern>d.MM.y.</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<dateTimeFormats>
					<intervalFormats>
						<intervalFormatItem id="MMMd">
							<greatestDifference id="d" draft="unconfirmed">d.MM - d.MM</greatestDifference>
							<greatestDifference id="M" draft="unconfirmed">d.MM - d.MM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MMMEd">
							<greatestDifference id="d" draft="unconfirmed">E, d.MM - E, d.MM</greatestDifference>
							<greatestDifference id="M" draft="unconfirmed">E, d.MM - E, d.MM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMM">
							<greatestDifference id="M" draft="unconfirmed">MM.y - MM.y</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">MM.y - MM.y</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMMd">
							<greatestDifference id="d" draft="unconfirmed">d.MM.y. - d.MM.y.</greatestDifference>
							<greatestDifference id="M" draft="unconfirmed">d.MM.y. - d.MM.y.</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">d.MM.y. - d.MM.y.</greatestDifference>
						</intervalFormatItem>
					</intervalFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>
