<ldml>
    <rbnf>
        <ruleset type="spellout_year">
            <rbnfrule value="-x">minus →→;</rbnfrule>
            <rbnfrule value="0">zero; one; two; three; four; five; six; seven; eigth; nine;</rbnfrule>
            <rbnfrule value="10">ten; eleven; twelve; thirteen; fourteen; fifteen; sixteen; seventeen; eighteen; nineteen;</rbnfrule>
            <rbnfrule value="20">twenty[-→→];</rbnfrule>
            <rbnfrule value="30">thirty[-→→];</rbnfrule>
