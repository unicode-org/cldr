            <rbnfrule value="0">th; st; nd; rd; th;</rbnfrule>
            <rbnfrule value="20">→→;</rbnfrule>
            <rbnfrule value="100">→→;</rbnfrule>
        </ruleset>
    </rbnf>
</ldml>
