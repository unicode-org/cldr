            <rbnfrule value="80">eighty[-→→];</rbnfrule>
            <rbnfrule value="90">ninety[-→→];</rbnfrule>
            <rbnfrule value="100">←← hundred[ →→];</rbnfrule>
            <rbnfrule value="1000">←← thousand[ →→];</rbnfrule>
            <rbnfrule value="1000000">←← million[ →→];</rbnfrule>
