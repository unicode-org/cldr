<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "../../common/dtd/ldml.dtd">
<ldml>
    <identity>
        <version number="$Revision$"/>
        <language type="my"/>
    </identity>
    <rbnf>
        <rulesetGrouping type="SpelloutRules">
            <rbnfRules><![CDATA[
%spellout-numbering-year:
x.x: =0.0=;
0: =%spellout-cardinal=;
%spellout-numbering:
0: =%spellout-cardinal=;
%spellout-cardinal:
-x: အနုတ်>>;
x.x: <<ဒသမ>>;
0: သုည;
1: တစ်;
2: နှစ်;
3: သုံး;
4: လေး;
5: ငါး;
6: ခြောက်;
7: ခုနှစ်;
8: ရှစ်;
9: ကိုး;
10: ဆယ်;
11: ဆယ့်[>>];
20: <<ဆယ်[>>];
100: <<ရာ;
101: <<ရာ့[>>];
1000: <<ထောင်;
1001: <<ထောင့်[>>];
10000: <<သောင်း[>>];
100000: <<သိန်း[>>];
1000000: <<သန်း[>>];
10000000: <<ကုဋေ[>>];
100000000000000: <<ကောဋိ[>>];
1000000000000000000: =#,##0=;
%spellout-ordinal:
-x: အနုတ်>>;
x.x: =#,##0.#=;
0: =%spellout-cardinal=;
1: ပထမ;
2: ဒုတိယ;
3: တတိယ;
4: စတုတ္ထ;
5: ပဉ္စမ;
6: ဆဋ္ဌမ;
7: သတ္တမ;
8: အဋ္ဌမ;
9: နဝမ;
10: ဒသမ;
11: =%spellout-cardinal=;
]]></rbnfRules>
            <!-- The following redundant ruleset elements have been deprecated and will be removed in the next release. Please use the rbnfRules contents instead. -->
            <ruleset type="spellout-numbering-year">
                <rbnfrule value="x.x">=0.0=;</rbnfrule>
                <rbnfrule value="0">=%spellout-cardinal=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-numbering">
                <rbnfrule value="0">=%spellout-cardinal=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-cardinal">
                <rbnfrule value="-x">အနုတ်→→;</rbnfrule>
                <rbnfrule value="x.x">←←ဒသမ→→;</rbnfrule>
                <rbnfrule value="0">သုည;</rbnfrule>
                <rbnfrule value="1">တစ်;</rbnfrule>
                <rbnfrule value="2">နှစ်;</rbnfrule>
                <rbnfrule value="3">သုံး;</rbnfrule>
                <rbnfrule value="4">လေး;</rbnfrule>
                <rbnfrule value="5">ငါး;</rbnfrule>
                <rbnfrule value="6">ခြောက်;</rbnfrule>
                <rbnfrule value="7">ခုနှစ်;</rbnfrule>
                <rbnfrule value="8">ရှစ်;</rbnfrule>
                <rbnfrule value="9">ကိုး;</rbnfrule>
                <rbnfrule value="10">ဆယ်;</rbnfrule>
                <rbnfrule value="11">ဆယ့်[→→];</rbnfrule>
                <rbnfrule value="20">←←ဆယ်[→→];</rbnfrule>
                <rbnfrule value="100">←←ရာ;</rbnfrule>
                <rbnfrule value="101">←←ရာ့[→→];</rbnfrule>
                <rbnfrule value="1000">←←ထောင်;</rbnfrule>
                <rbnfrule value="1001">←←ထောင့်[→→];</rbnfrule>
                <rbnfrule value="10000">←←သောင်း[→→];</rbnfrule>
                <rbnfrule value="100000">←←သိန်း[→→];</rbnfrule>
                <rbnfrule value="1000000">←←သန်း[→→];</rbnfrule>
                <rbnfrule value="10000000">←←ကုဋေ[→→];</rbnfrule>
                <rbnfrule value="100000000000000">←←ကောဋိ[→→];</rbnfrule>
                <rbnfrule value="1000000000000000000">=#,##0=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-ordinal">
                <rbnfrule value="-x">အနုတ်→→;</rbnfrule>
                <rbnfrule value="x.x">=#,##0.#=;</rbnfrule>
                <rbnfrule value="0">=%spellout-cardinal=;</rbnfrule>
                <rbnfrule value="1">ပထမ;</rbnfrule>
                <rbnfrule value="2">ဒုတိယ;</rbnfrule>
                <rbnfrule value="3">တတိယ;</rbnfrule>
                <rbnfrule value="4">စတုတ္ထ;</rbnfrule>
                <rbnfrule value="5">ပဉ္စမ;</rbnfrule>
                <rbnfrule value="6">ဆဋ္ဌမ;</rbnfrule>
                <rbnfrule value="7">သတ္တမ;</rbnfrule>
                <rbnfrule value="8">အဋ္ဌမ;</rbnfrule>
                <rbnfrule value="9">နဝမ;</rbnfrule>
                <rbnfrule value="10">ဒသမ;</rbnfrule>
                <rbnfrule value="11">=%spellout-cardinal=;</rbnfrule>
            </ruleset>
        </rulesetGrouping>
    </rbnf>
</ldml>
