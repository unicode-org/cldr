<ldml>
    <identity>
        <version number="$Revision$"/>
        <generation date="$Date$"/>
        <language type="pl"/>
    </identity>
    <rbnf>
        <rulesetGrouping type="SpelloutRules">
            <ruleset type="spellout-numbering-year">
                <rbnfrule value="x.x">=#,###0.#=;</rbnfrule>
                <rbnfrule value="0">=%spellout-numbering=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-numbering">
                <rbnfrule value="0">=%spellout-cardinal-masculine=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-cardinal-masculine">
                <rbnfrule value="-x">minus →→;</rbnfrule>
                <rbnfrule value="x.x">←← przecinek →→;</rbnfrule>
                <rbnfrule value="0">zero;</rbnfrule>
                <rbnfrule value="1">jeden;</rbnfrule>
                <rbnfrule value="2">dwa;</rbnfrule>
                <rbnfrule value="3">trzy;</rbnfrule>
                <rbnfrule value="4">cztery;</rbnfrule>
                <rbnfrule value="5">pięć;</rbnfrule>
                <rbnfrule value="6">sześć;</rbnfrule>
                <rbnfrule value="7">siedem;</rbnfrule>
                <rbnfrule value="8">osiem;</rbnfrule>
                <rbnfrule value="9">dziewięć;</rbnfrule>
                <rbnfrule value="10">dziesięć;</rbnfrule>
                <rbnfrule value="11">jedenaście;</rbnfrule>
                <rbnfrule value="12">dwanaście;</rbnfrule>
                <rbnfrule value="13">trzynaście;</rbnfrule>
                <rbnfrule value="14">czternaście;</rbnfrule>
                <rbnfrule value="15">piętnaście;</rbnfrule>
                <rbnfrule value="16">szesnaście;</rbnfrule>
                <rbnfrule value="17">siedemnaście;</rbnfrule>
                <rbnfrule value="18">osiemnaście;</rbnfrule>
                <rbnfrule value="19">dziewiętnaście;</rbnfrule>
                <rbnfrule value="20">dwadzieścia[ →→];</rbnfrule>
                <rbnfrule value="30">trzydzieści[ →→];</rbnfrule>
                <rbnfrule value="40">czterdzieści[ →→];</rbnfrule>
                <rbnfrule value="50">←←dziesiąt[ →→];</rbnfrule>
                <rbnfrule value="100">sto[ →→];</rbnfrule>
                <rbnfrule value="200">dwieście[ →→];</rbnfrule>
                <rbnfrule value="300">←←sta[ →→];</rbnfrule>
                <rbnfrule value="500">←←set[ →→];</rbnfrule>
                <rbnfrule value="1000">←%spellout-cardinal-masculine←$(cardinal,one{ tysiąc}few{ tysiące}other{ tysięcy})[ →→];</rbnfrule>
                <rbnfrule value="1000000">←%spellout-cardinal-masculine←$(cardinal,one{ milion}few{ miliony}other{ milionów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000">←%spellout-cardinal-masculine←$(cardinal,one{ miliard}few{ miliardy}other{ miliardów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000000">←%spellout-cardinal-masculine←$(cardinal,one{ bilion}few{ biliony}other{ bilionów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000000000">←%spellout-cardinal-masculine←$(cardinal,one{ biliard}few{ biliardy}other{ biliardów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000000000000">=#,##0=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-cardinal-neuter">
                <rbnfrule value="-x">minus →→;</rbnfrule>
                <rbnfrule value="x.x">←← przecinek →%%spellout-fraction→;</rbnfrule>
                <rbnfrule value="0">zero;</rbnfrule>
                <rbnfrule value="1">jedno;</rbnfrule>
                <rbnfrule value="2">=%spellout-cardinal-masculine=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-cardinal-feminine">
                <rbnfrule value="-x">minus →→;</rbnfrule>
                <rbnfrule value="x.x">←← przecinek →%%spellout-fraction→;</rbnfrule>
                <rbnfrule value="0">zero;</rbnfrule>
                <rbnfrule value="1">jedna;</rbnfrule>
                <rbnfrule value="2">=%%spellout-cardinal-feminine-priv=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-cardinal-feminine-priv" access="private">
                <rbnfrule value="1">jeden;</rbnfrule>
                <rbnfrule value="2">dwie;</rbnfrule>
                <rbnfrule value="3">=%spellout-cardinal-masculine=;</rbnfrule>
                <rbnfrule value="20">dwadzieścia[ →→];</rbnfrule>
                <rbnfrule value="30">trzydzieści[ →→];</rbnfrule>
                <rbnfrule value="40">czterdzieści[ →→];</rbnfrule>
                <rbnfrule value="50">←←dziesiąt[ →→];</rbnfrule>
                <rbnfrule value="100">sto[ →→];</rbnfrule>
                <rbnfrule value="200">dwieście[ →→];</rbnfrule>
                <rbnfrule value="300">←←sta[ →→];</rbnfrule>
                <rbnfrule value="500">←←set[ →→];</rbnfrule>
                <rbnfrule value="1000">←%spellout-cardinal-masculine←$(cardinal,one{ tysiąc}few{ tysiące}other{ tysięcy})[ →→];</rbnfrule>
                <rbnfrule value="1000000">←%spellout-cardinal-masculine←$(cardinal,one{ milion}few{ miliony}other{ milionów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000">←%spellout-cardinal-masculine←$(cardinal,one{ miliard}few{ miliardy}other{ miliardów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000000">←%spellout-cardinal-masculine←$(cardinal,one{ bilion}few{ biliony}other{ bilionów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000000000">←%spellout-cardinal-masculine←$(cardinal,one{ biliard}few{ biliardy}other{ biliardów})[ →→];</rbnfrule>
                <rbnfrule value="1000000000000000000">=#,##0=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-fraction" access="private">
                <rbnfrule value="10">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="100">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="1000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="10000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="100000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="1000000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="10000000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="100000000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="1000000000">←%%spellout-fraction-digits←←;</rbnfrule>
                <rbnfrule value="10000000000">←0←;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-fraction-digits" access="private">
                <rbnfrule value="0">=%spellout-cardinal-masculine=;</rbnfrule>
                <rbnfrule value="10">←← →→;</rbnfrule>
            </ruleset>
        </rulesetGrouping>
    </rbnf>
</ldml>
