<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="en"/>
        <territory type="US"/>
        <variant type="POSIX"/>
	</identity>
	<segmentations>
		<segmentation type="WordBreak">
			<variables>
				<variable id="$MidLetter">[$MidLetter-\:-\,]</variable>
			</variables>
		</segmentation>
	</segmentations>
</ldml>


