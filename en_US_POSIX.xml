<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.3/ldml.dtd">
<ldml>
	<!-- This is a special purpose locale so that a user can request the C or POSIX
		 style formatting. This does not include grouping separators in numbers, nor
		 unusual characters for infinity or NaN. Some people have known this to be the
		 invariant locale, which is different from the root locale.

		 This locale should not be modified to create some sort of custom collation to
		 represent a specific codepage. Many users complained when the C locale was
		 being used, and multi-level UCA collation was expected.
	-->
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="en"/>
		<territory type="US"/>
		<variant type="POSIX"/>
	</identity>
	<numbers>
		<symbols>
			<perMille>0/00</perMille>
			<infinity>INF</infinity>
			<nan>NaN</nan>
		</symbols>
		<decimalFormats>
			<decimalFormatLength>
				<decimalFormat>
					<pattern>#0.######</pattern>
				</decimalFormat>
			</decimalFormatLength>
		</decimalFormats>
		<scientificFormats>
			<scientificFormatLength>
				<scientificFormat>
					<pattern>0.000000E+000</pattern>
				</scientificFormat>
			</scientificFormatLength>
		</scientificFormats>
		<percentFormats>
			<percentFormatLength>
				<percentFormat>
					<pattern>#0%</pattern>
				</percentFormat>
			</percentFormatLength>
		</percentFormats>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>Â¤ #0.00</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>
