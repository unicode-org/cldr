<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.6/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="en"/>
		<territory type="ZA"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="full">
						<dateFormat>
							<pattern>EEEE dd MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="long">
						<dateFormat>
							<pattern>dd MMMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="medium">
						<dateFormat>
							<pattern>dd MMM yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>yyyy/MM/dd</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern alt="proposed-x1001" draft="unconfirmed">h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern alt="proposed-x1001" draft="unconfirmed">h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats>
						<dateFormatItem id="MMdd" draft="unconfirmed">MM/dd</dateFormatItem>
						<dateFormatItem id="yyyyMM" draft="unconfirmed">yyyy/MM</dateFormatItem>
						<dateFormatItem id="yyyyMMMM" draft="unconfirmed">MMMM yyyy</dateFormatItem>
					</availableFormats>
					<intervalFormats>
						<intervalFormatFallback draft="unconfirmed">{0} - {1}</intervalFormatFallback>
						<intervalFormatItem id="M">
							<greatestDifference id="M" draft="unconfirmed">M-M</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MEEEd">
							<greatestDifference id="M" draft="unconfirmed">EEE MM/dd - EEE MM/dd</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">EEE MM/dd - EEE MM/dd</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MMM">
							<greatestDifference id="M" draft="unconfirmed">MMM-MMM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MMMEEEd">
							<greatestDifference id="M" draft="unconfirmed">EEE dd MMM - EEE dd MMM</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">EEE dd - EEE dd MMM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="MMMd">
							<greatestDifference id="M" draft="unconfirmed">dd MMM - dd MMM</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">dd-dd MMM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="Md">
							<greatestDifference id="M" draft="unconfirmed">MM/dd - MM/dd</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">MM/dd - MM/dd</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="d">
							<greatestDifference id="d" draft="unconfirmed">d-d</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="h">
							<greatestDifference id="a" draft="unconfirmed">h a - h a</greatestDifference>
							<greatestDifference id="h" draft="unconfirmed">h-h a</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="hm">
							<greatestDifference id="a" draft="unconfirmed">h:mm a - h:mm a</greatestDifference>
							<greatestDifference id="h" draft="unconfirmed">h:mm-h:mm a</greatestDifference>
							<greatestDifference id="m" draft="unconfirmed">h:mm-h:mm a</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="hmv">
							<greatestDifference id="a" draft="unconfirmed">h:mm a - h:mm a v</greatestDifference>
							<greatestDifference id="h" draft="unconfirmed">h:mm-h:mm a v</greatestDifference>
							<greatestDifference id="m" draft="unconfirmed">h:mm-h:mm a v</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="hv">
							<greatestDifference id="a" draft="unconfirmed">h a - h a v</greatestDifference>
							<greatestDifference id="h" draft="unconfirmed">h-h a v</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="y">
							<greatestDifference id="y" draft="unconfirmed">y-y</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yM">
							<greatestDifference id="M" draft="unconfirmed">yyyy/MM - yyyy/MM</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">yyyy/MM - yyyy/MM</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMEEEd">
							<greatestDifference id="M" draft="unconfirmed">EEE yyyy/MM/dd - EEE yyyy/MM/dd</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">EEE yyyy/MM/dd - EEE yyyy/MM/dd</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">EEE yyyy/MM/dd - EEE yyyy/MM/dd</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMM">
							<greatestDifference id="M" draft="unconfirmed">MMM-MMM yyyy</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">MMM yyyy - MMM yyyy</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMMEEEd">
							<greatestDifference id="M" draft="unconfirmed">EEE dd MMM - EEE dd MMM yyyy</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">EEE dd - EEE dd MMM yyyy</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">EEE dd MMM yyyy - EEE dd MMM yyyy</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMMMd">
							<greatestDifference id="M" draft="unconfirmed">dd MMM - dd MMM yyyy</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">dd-dd MMM yyyy</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">dd MMM yyyy - dd MMM yyyy</greatestDifference>
						</intervalFormatItem>
						<intervalFormatItem id="yMd">
							<greatestDifference id="M" draft="unconfirmed">yyyy/MM/dd - yyyy/MM/dd</greatestDifference>
							<greatestDifference id="d" draft="unconfirmed">yyyy/MM/dd - yyyy/MM/dd</greatestDifference>
							<greatestDifference id="y" draft="unconfirmed">yyyy/MM/dd - yyyy/MM/dd</greatestDifference>
						</intervalFormatItem>
					</intervalFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
		<timeZoneNames>
			<metazone type="Africa_Central">
				<commonlyUsed>true</commonlyUsed>
			</metazone>
			<metazone type="Africa_Eastern">
				<commonlyUsed>true</commonlyUsed>
			</metazone>
			<metazone type="Africa_Southern">
				<commonlyUsed>true</commonlyUsed>
			</metazone>
			<metazone type="Africa_Western">
				<commonlyUsed>true</commonlyUsed>
			</metazone>
		</timeZoneNames>
	</dates>
	<numbers>
		<symbols>
			<decimal draft="provisional">,</decimal>
			<group draft="provisional"> </group>
		</symbols>
		<currencyFormats>
			<currencyFormatLength>
				<currencyFormat>
					<pattern>¤#,##0.00</pattern>
				</currencyFormat>
			</currencyFormatLength>
		</currencyFormats>
	</numbers>
</ldml>

