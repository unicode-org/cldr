<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.4/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="es"/>
		<territory type="BO"/>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<dateFormats>
					<dateFormatLength type="medium">
						<dateFormat>
							<pattern>dd/MM/yyyy</pattern>
						</dateFormat>
					</dateFormatLength>
					<dateFormatLength type="short">
						<dateFormat>
							<pattern>dd/MM/yy</pattern>
						</dateFormat>
					</dateFormatLength>
				</dateFormats>
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>hh:mm:ss a z</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern>hh:mm:ss a z</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="medium">
						<timeFormat>
							<pattern>hh:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="short">
						<timeFormat>
							<pattern>hh:mm a</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
				<dateTimeFormats>
					<availableFormats draft="true">
						<dateFormatItem id="1">MM-dd</dateFormatItem>
						<dateFormatItem id="2">d. MMMM yyyy</dateFormatItem>
						<dateFormatItem id="3">d. MMM. yyyy</dateFormatItem>
						<dateFormatItem id="4">EEEE,d 'de' MMMM 'de' yyyy</dateFormatItem>
						<dateFormatItem id="5">EEE d 'de' MMMM 'de' yyyy</dateFormatItem>
						<dateFormatItem id="6">EEE d 'de' MMM 'de' yy</dateFormatItem>
						<dateFormatItem id="7">d 'de' MMMM 'de' yyyy</dateFormatItem>
						<dateFormatItem id="8">d 'de' MMM 'de' yyyy</dateFormatItem>
						<dateFormatItem id="9">d 'de' MMM 'de' yy</dateFormatItem>
						<dateFormatItem id="10">dd/MM/yy</dateFormatItem>
						<dateFormatItem id="11">dd/MM/yyyy</dateFormatItem>
						<dateFormatItem id="12">QQQQ yy</dateFormatItem>
						<dateFormatItem id="13">MMMM</dateFormatItem>
						<dateFormatItem id="14">dd/MMM</dateFormatItem>
						<dateFormatItem id="15">MM/yy</dateFormatItem>
						<dateFormatItem id="16">EEE dd/MMM yy</dateFormatItem>
						<dateFormatItem id="17">d/MM/yy</dateFormatItem>
						<dateFormatItem id="18">ww</dateFormatItem>
						<dateFormatItem id="19">yyyy-MM-dd</dateFormatItem>
						<dateFormatItem id="20">yy-MM-dd</dateFormatItem>
						<dateFormatItem id="21">mm:ss.SS</dateFormatItem>
						<dateFormatItem id="22">KK:mm:ss a</dateFormatItem>
						<dateFormatItem id="23">KK:mm a</dateFormatItem>
						<dateFormatItem id="24">HH:mm:ss</dateFormatItem>
						<dateFormatItem id="25">HH:mm</dateFormatItem>
						<dateFormatItem id="26">dd/MM/yyyy HH:mm:ss</dateFormatItem>
						<dateFormatItem id="27">dd/MM/yy HH:mm</dateFormatItem>
					</availableFormats>
				</dateTimeFormats>
			</calendar>
		</calendars>
	</dates>
</ldml>

