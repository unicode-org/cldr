<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="ckb-t-k0-android">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Central Kurdish"/>
	</names>
	<keyMap>
		<map iso="D01" to="ق"/> <!-- Q -->
		<map iso="D02" to="و" longPress="وو"/> <!-- W -->
		<map iso="D03" to="ە"/> <!-- E -->
		<map iso="D04" to="ر" longPress="ڕ"/> <!-- R -->
		<map iso="D05" to="ت" longPress="ط"/><!-- T -->
		<map iso="D06" to="ی"/> <!-- Y -->
		<map iso="D07" to="ئ"/> <!-- U -->
		<map iso="D08" to="ح" longPress="ع"/> <!-- I -->
		<map iso="D09" to="ۆ"/> <!-- O -->
		<map iso="D10" to="پ" longPress="ث"/><!-- P -->
		<map iso="C01" to="ا" longPress="آ"/> <!-- A -->
		<map iso="C02" to="س" longPress="ش"/><!-- S -->
		<map iso="C03" to="د" longPress="ذ"/> <!-- D -->
		<map iso="C04" to="ف" longPress="ظ"/> <!-- F -->
		<map iso="C05" to="گ" longPress="غ"/> <!-- G -->
		<map iso="C06" to="ه"/> <!-- H -->
		<map iso="C07" to="ژ"/> <!-- J -->
		<map iso="C08" to="ک" longPress="ك"/><!-- K -->
		<map iso="C09" to="ل" longPress="ڵ"/> <!-- L -->
		<map iso="B01" to="ز" longPress="ض"/><!-- Z -->
		<map iso="B02" to="خ" longPress="ص"/><!-- X -->
		<map iso="B03" to="ج" longPress="چ"/><!-- C -->
		<map iso="B04" to="ڤ"/> <!-- V -->
		<map iso="B05" to="ب"/> <!-- B -->
		<map iso="B06" to="ن"/> <!-- N -->
		<map iso="B07" to="م"/> <!-- M -->
		<map iso="A02" to=","/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="\u{22} ' # - : ! ، ؟ @ &amp; % + ؛ / ) ("/> <!-- (key to right of space)  long=" ' # - : ! ، ؟ @ & % + ؛ / ) (  -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="١" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q  base=ق -->
		<map iso="D02" to="٢" longPress="² ⅔"/> <!-- W  base=و -->
		<map iso="D03" to="٣" longPress="³ ¾ ⅜"/> <!-- E  base=ە -->
		<map iso="D04" to="٤" longPress="⁴"/> <!-- R  base=ر -->
		<map iso="D05" to="٥" longPress="⅝"/> <!-- T  base=ت -->
		<map iso="D06" to="٦"/> <!-- Y  base=ی -->
		<map iso="D07" to="٧" longPress="⅞"/> <!-- U  base=ئ -->
		<map iso="D08" to="٨"/> <!-- I  base=ح -->
		<map iso="D09" to="٩"/> <!-- O  base=ۆ -->
		<map iso="D10" to="٠" longPress="ⁿ ∅"/> <!-- P  base=پ -->
		<map iso="C01" to="@"/> <!-- A  base=ا -->
		<map iso="C02" to="#"/> <!-- S  base=س -->
		<map iso="C03" to="$" longPress="¢ £ € ¥ ₱"/> <!-- D  base=د -->
		<map iso="C04" to="٪" longPress="% ‰"/> <!-- F  base=ف -->
		<map iso="C05" to="&amp;"/> <!-- G  base=گ -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H  base=ه -->
		<map iso="C07" to="+" longPress="±"/> <!-- J  base=ژ -->
		<map iso="C08" to="(" longPress="﴿ &gt; } ]"/> <!-- K  base=ک -->
		<map iso="C09" to=")" longPress="﴾ &lt; { ["/> <!-- L  base=ل -->
		<map iso="B01" to="*" longPress="★ ٭"/> <!-- Z  base=ز -->
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X  base=خ  to= "  -->
		<map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C  base=ج -->
		<map iso="B04" to=":"/> <!-- V  base=ڤ -->
		<map iso="B05" to="؛" longPress=";"/> <!-- B  base=ب -->
		<map iso="B06" to="!" longPress="¡"/> <!-- N  base=ن -->
		<map iso="B07" to="؟" longPress="?"/> <!-- M  base=م -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=، -->
		<map iso="A03" to="،" longPress="؟ ؛ ! : - / ' \u{22}"/> <!-- space  long=؟ ؛ ! : - / ' "  -->
		<map iso="A04" to="." longPress="\u{655} \u{654} \u{652} \u{64D} \u{64C} \u{64B} \u{651} \u{656} \u{670} \u{653} \u{650} \u{64F} \u{64E} ـ"/> <!-- (key to right of space)  long= ٕ  ٔ  ْ  ٍ  ٌ  ً  ّ  ٖ  ٰ  ٓ  ِ  ُ  َ ـ  -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q  base=ق -->
		<map iso="D02" to="`"/> <!-- W  base=و -->
		<map iso="D03" to="|"/> <!-- E  base=ە -->
		<map iso="D04" to="•" longPress="♪"/> <!-- R  base=ر -->
		<map iso="D05" to="√"/> <!-- T  base=ت -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y  base=ی -->
		<map iso="D07" to="÷"/> <!-- U  base=ئ -->
		<map iso="D08" to="×"/> <!-- I  base=ح -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O  base=ۆ -->
		<map iso="D10" to="∆"/> <!-- P  base=پ -->
		<map iso="C01" to="£"/> <!-- A  base=ا -->
		<map iso="C02" to="¢"/> <!-- S  base=س -->
		<map iso="C03" to="€"/> <!-- D  base=د -->
		<map iso="C04" to="¥"/> <!-- F  base=ف -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G  base=گ -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H  base=ه -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J  base=ژ -->
		<map iso="C08" to="{"/> <!-- K  base=ک -->
		<map iso="C09" to="}"/> <!-- L  base=ل -->
		<map iso="B01" to="\"/> <!-- Z  base=ز -->
		<map iso="B02" to="©"/> <!-- X  base=خ -->
		<map iso="B03" to="®"/> <!-- C  base=ج -->
		<map iso="B04" to="™"/> <!-- V  base=ڤ -->
		<map iso="B05" to="℅"/> <!-- B  base=ب -->
		<map iso="B06" to="["/> <!-- N  base=ن -->
		<map iso="B07" to="]"/> <!-- M  base=م -->
		<map iso="A00" to="&lt;" longPress="› ≥ »"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="‹ ≤ «"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=، -->
		<map iso="A03" to="،" longPress="؟ ؛ ! : - / ' \u{22}"/> <!-- space  long=؟ ؛ ! : - / ' "  -->
		<map iso="A04" to="." longPress="\u{655} \u{654} \u{652} \u{64D} \u{64C} \u{64B} \u{651} \u{656} \u{670} \u{653} \u{650} \u{64F} \u{64E} ـ"/> <!-- (key to right of space)  long= ٕ  ٔ  ْ  ٍ  ٌ  ً  ّ  ٖ  ٰ  ٓ  ِ  ُ  َ ـ  -->
	</keyMap>
</keyboard>
