<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="kk-Arab-t-k0-android-768dpi">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Kazakh Arabic (768dpi)"/>
	</names>
	<keyMap>
		<map iso="D01" to="ق"/> <!-- Q -->
		<map iso="D02" to="ۋ"/> <!-- W -->
		<map iso="D03" to="ە"/> <!-- E -->
		<map iso="D04" to="ر"/> <!-- R -->
		<map iso="D05" to="ت"/> <!-- T -->
		<map iso="D06" to="ي"/> <!-- Y -->
		<map iso="D07" to="ۇ"/> <!-- U -->
		<map iso="D08" to="ى"/> <!-- I -->
		<map iso="D09" to="و"/> <!-- O -->
		<map iso="D10" to="پ"/> <!-- P -->
		<map iso="D11" to="ع"/> <!-- [ -->
		<map iso="C01" to="ا"/> <!-- A -->
		<map iso="C02" to="س"/> <!-- S -->
		<map iso="C03" to="د"/> <!-- D -->
		<map iso="C04" to="ف"/> <!-- F -->
		<map iso="C05" to="گ"/> <!-- G -->
		<map iso="C06" to="ح"/> <!-- H -->
		<map iso="C07" to="ج"/> <!-- J -->
		<map iso="C08" to="ك"/> <!-- K -->
		<map iso="C09" to="ل"/> <!-- L -->
		<map iso="C10" to="ڭ"/> <!-- ; -->
		<map iso="C11" to="ھ"/> <!-- ' -->
		<map iso="B01" to="ز"/> <!-- Z -->
		<map iso="B02" to="ش"/> <!-- X -->
		<map iso="B03" to="چ"/> <!-- C -->
		<map iso="B04" to="ۆ"/> <!-- V -->
		<map iso="B05" to="ب"/> <!-- B -->
		<map iso="B06" to="ن"/> <!-- N -->
		<map iso="B07" to="م"/> <!-- M -->
		<map iso="B08" to="ء"/> <!-- , -->
		<map iso="B09" to="؟" longPress="!"/> <!-- . -->
		<map iso="A02" to="،"/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="\u{22} ' # - : ! ، ؟ @ &amp; % + ؛ / ) ("/> <!-- (key to right of space)  long=" ' # - : ! ، ؟ @ & % + ؛ / ) (  -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q base=ق -->
		<map iso="D02" to="2" longPress="² ⅔"/> <!-- W base=ۋ -->
		<map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E base=ە -->
		<map iso="D04" to="4" longPress="⁴"/> <!-- R base=ر -->
		<map iso="D05" to="5" longPress="⅝"/> <!-- T base=ت -->
		<map iso="D06" to="6"/> <!-- Y base=ي -->
		<map iso="D07" to="7" longPress="⅞"/> <!-- U base=ۇ -->
		<map iso="D08" to="8"/> <!-- I base=ى -->
		<map iso="D09" to="9"/> <!-- O base=و -->
		<map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P base=پ -->
		<map iso="C01" to="@"/> <!-- A base=ا -->
		<map iso="C02" to="#"/> <!-- S base=س -->
		<map iso="C03" to="₸" longPress="$ ¢ £ € ¥ ₱"/> <!-- D base=د -->
		<map iso="C04" to="%" longPress="‰"/> <!-- F base=ف -->
		<map iso="C05" to="&amp;"/> <!-- G base=گ -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H base=ح -->
		<map iso="C07" to="+" longPress="±"/> <!-- J base=ج -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K base=ك -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L base=ل -->
		<map iso="B01" to="*" longPress="† ‡ ★"/> <!-- Z base=ز -->
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X base=ش  to= "  -->
		<map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C base=چ -->
		<map iso="B04" to=":"/> <!-- V base=ۆ -->
		<map iso="B05" to="؛"/> <!-- B base=ب -->
		<map iso="B06" to="!" longPress="¡"/> <!-- N base=ن -->
		<map iso="B07" to="؟" longPress="?"/> <!-- M base=م -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=، -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="."/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q base=ق -->
		<map iso="D02" to="`"/> <!-- W base=ۋ -->
		<map iso="D03" to="|"/> <!-- E base=ە -->
		<map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R base=ر -->
		<map iso="D05" to="√"/> <!-- T base=ت -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y base=ي -->
		<map iso="D07" to="÷"/> <!-- U base=ۇ -->
		<map iso="D08" to="×"/> <!-- I base=ى -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O base=و -->
		<map iso="D10" to="∆"/> <!-- P base=پ -->
		<map iso="C01" to="£"/> <!-- A base=ا -->
		<map iso="C02" to="¢"/> <!-- S base=س -->
		<map iso="C03" to="€"/> <!-- D base=د -->
		<map iso="C04" to="¥"/> <!-- F base=ف -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G base=گ -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H base=ح -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J base=ج -->
		<map iso="C08" to="{"/> <!-- K base=ك -->
		<map iso="C09" to="}"/> <!-- L base=ل -->
		<map iso="B01" to="\"/> <!-- Z base=ز -->
		<map iso="B02" to="©"/> <!-- X base=ش -->
		<map iso="B03" to="®"/> <!-- C base=چ -->
		<map iso="B04" to="™"/> <!-- V base=ۆ -->
		<map iso="B05" to="℅"/> <!-- B base=ب -->
		<map iso="B06" to="["/> <!-- N base=ن -->
		<map iso="B07" to="]"/> <!-- M base=م -->
		<map iso="A00" to="&lt;" longPress="› ≥ »"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="‹ ≤ «"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=، -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="."/> <!-- (key to right of space) -->
	</keyMap>
</keyboard>
