<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboardTest SYSTEM "../dtd/ldmlKeyboardTest.dtd">
<keyboardTest conformsTo="techpreview">
    <info keyboard="ja-Latn.xml" author="Team Keyboard" name="ja-Latn-test" />
    <repertoire name="latn-repertoire" chars="[a-z A-Z 0-9 !\u0022#$%\u0026\[\]\{\}=\-|Â¥~\^_\u0020\u003c>,./?`@\+\*]" type="simple" />
    <tests name="tests">
        <test name="test1">
			<startContext to="" /> <!-- empty startContext -->
			<keystroke key="n"/>
			<keystroke key="m"/>
			<keystroke key="comma"/>
			<keystroke key="period"/>
			<keystroke key="slash"/>
			<check result="nm,./"/>
        </test>
        <test name="test2">
			<startContext to="" /> <!-- empty startContext -->
			<keystroke key="open-square"/>
			<keystroke key="8"/>
			<keystroke key="9"/>
			<keystroke key="0"/>
			<keystroke key="pipe"/>
			<check result="[890|"/>
        </test>
    </tests>
</keyboardTest>
