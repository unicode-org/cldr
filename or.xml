<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ldml SYSTEM "http://www.unicode.org/cldr/dtd/1.3/ldml.dtd">
<ldml>
	<identity>
		<version number="$Revision$"/>
		<generation date="$Date$"/>
		<language type="or"/>
	</identity>
	<localeDisplayNames>
		<languages>
			<language type="or" draft="true">ଓଡ଼ିଆ</language>
		</languages>
		<territories>
