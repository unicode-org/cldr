	</characters>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<months>
					<monthContext type="format">
						<monthWidth type="wide">
							<month type="1" draft="true">ଜାନୁଆରୀ</month>
							<month type="2" draft="true">ଫେବ୍ରୁୟାରୀ</month>
