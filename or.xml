						</monthWidth>
					</monthContext>
				</months>
				<days>
					<dayContext type="format">
						<dayWidth type="abbreviated">
							<day type="sun" draft="true">ରବି</day>
							<day type="mon" draft="true">ସୋମ</day>
							<day type="tue" draft="true">ମଙ୍ଗଳ</day>
