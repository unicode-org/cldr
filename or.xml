							<day type="fri" draft="true">ଶୁକ୍ର</day>
							<day type="sat" draft="true">ଶନି</day>
						</dayWidth>
						<dayWidth type="wide">
							<day type="sun" draft="true">ରବିବାର</day>
							<day type="mon" draft="true">ସୋମବାର</day>
							<day type="tue" draft="true">ମଙ୍ଗଳବାର</day>
							<day type="wed" draft="true">ବୁଧବାର</day>
							<day type="thu" draft="true">ଗୁରୁବାର</day>
							<day type="fri" draft="true">ଶୁକ୍ରବାର</day>
							<day type="sat" draft="true">ଶନିବାର</day>
						</dayWidth>
					</dayContext>
				</days>
			</calendar>
		</calendars>
	</dates>
	<numbers>
		<symbols>
			<nativeZeroDigit draft="true">୦</nativeZeroDigit>
		</symbols>
	</numbers>
	<posix>
		<messages>
