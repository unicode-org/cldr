<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE worldserverbundles [
<!ELEMENT worldserverbundles (worldserverbundle)*>
<!ATTLIST worldserverbundles date CDATA #REQUIRED>
<!ATTLIST worldserverbundles lazarus_id CDATA #REQUIRED>

<!ELEMENT worldserverbundle (msg)*>
<!ATTLIST worldserverbundle project_id CDATA #REQUIRED>
<!ATTLIST worldserverbundle message_count CDATA #REQUIRED>

<!ELEMENT msg (#PCDATA|ph)*>
<!ATTLIST msg xml:space CDATA #FIXED "preserve">
<!ATTLIST msg id CDATA #REQUIRED>
<!ATTLIST msg desc CDATA #IMPLIED>
<!ATTLIST msg context_internal CDATA #IMPLIED>
<!ATTLIST msg context_external CDATA #IMPLIED>

<!ELEMENT ph EMPTY>
<!ATTLIST ph name CDATA #REQUIRED>
<!ATTLIST ph ex CDATA #IMPLIED>
]>