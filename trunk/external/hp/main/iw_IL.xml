							<day type="sun">א'</day>
							<day type="mon">ב'</day>
							<day type="tue">ג'</day>
							<day type="wed">ד'</day>
							<day type="thu">ה'</day>
							<day type="fri">و'</day>
							<day type="sat">שבת</day>
